# AWS Cognito Configuration
# Copy this file to .env and fill in your actual values

# Your AWS Cognito User Pool ID (e.g., us-east-1_ABC123def)
AWS_COGNITO_USER_POOL_ID=your-user-pool-id-here

# Your AWS Cognito App Client ID (e.g., 1a2b3c4d5e6f7g8h9i0j1k2l3m)
AWS_COGNITO_CLIENT_ID=your-client-id-here

# AWS Region where your User Pool is located (e.g., us-east-1)
AWS_COGNITO_REGION=us-east-1

# LiveKit Configuration
# Your LiveKit server URL (e.g., wss://your-project.livekit.cloud)
LIVEKIT_URL=wss://your-livekit-server.livekit.cloud

# Your LiveKit API Key
LIVEKIT_API_KEY=your-api-key-here

# Your LiveKit API Secret
LIVEKIT_API_SECRET=your-api-secret-here
